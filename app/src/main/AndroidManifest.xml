<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.project.courier_app">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Courierapp"
        android:usesCleartextTraffic="true">
        <activity android:name=".activeDelivery"></activity>
        <activity android:name=".DetailsBank" />
        <activity android:name=".EditMyProfile" />
        <activity android:name=".ChangePassword" />
        <activity android:name=".ChooseDelivery" />
        <activity android:name=".CourierProfile" />
        <activity android:name=".RegisterNewCourier" />
        <activity android:name=".showChoosenDelivery" />
        <activity android:name=".DeliveryHistory" />
        <activity android:name=".CourierMain" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>